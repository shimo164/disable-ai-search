{
  "manifest_version": 3,
  "name": "Disable AI Search",
  "version": "1.0",
  "description": "Hide Google AI Overview results",
  "permissions": ["storage"],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["*://www.google.com/search*", "*://google.com/search*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ]
}
